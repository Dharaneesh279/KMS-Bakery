{
  "name": "kms-bakery-website",
  "version": "1.0.0",
  "description": "KMS Bakery Website with Node.js server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "keywords": ["bakery", "website", "express"],
  "author": "KMS Bakery",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "nodemonConfig": {
    "watch": ["*.js", "*.html", "*.css"],
    "ext": "js,html,css",
    "ignore": ["node_modules/*", "public/*"],
    "delay": "500"
  }
}